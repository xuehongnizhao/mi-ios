.mi-title {
	background-color: #5b3914;
}

.mui-badge {
	position: fixed;
	margin: 3px 10px;
	background: red;
	-webkit-border-radius: 16px;
	-moz-border-radius: 16px;
	border-radius: 16px;
	height: 16px;
	line-height: 16px;
	min-width: 16px;
	font-size: 12px;
}

.am-navbar .am-navbar-nav {
	background-color: #ffffff;
	border-top:1px solid #eee;
}

.am-navbar a img {
	width: 22px;
	height: auto;
	margin-top: 6px;
}

.am-navbar a span {
	color: #999999;
	font-size: 12px;
}
.bar-select{
	color: #ffcf58;
}
